<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.falconBus" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Falcon Bus</name>
    <description>
      A simple bus schedule app for the Desert Falcon Community
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
      Lingliang Zhang
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="fullscreen" value="true" />
    <preference name="webviewbounce" value="true" />
    <icon src="icon.png" />
    <gap:splash src="splash.png" />
</widget>
